{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/manage/(.*)",
        "dest": "/index.js"   
      },
      {
        "src": "/(.*)",       
        "dest": "/dist/$1"    
      }
    ]
  }
  